table
  width 100%
  border-collapse collapse
  font-size 12px
  tr:nth-child(odd)
    background #c8fff4
  td
    padding 5px 2px
    border-bottom 1px solid #f2f2f2
    vertical-align middle
  thead
    tr
      background white !important
    td
      font-size 12px
      font-weight bold

.subtitle
  padding 0
  margin 0
  color #666
  border-bottom 1px solid green

h1
  margin-bottom 0

img
  width 24px
  height 24px
  border-radius 12px
  vertical-align middle





